<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/DataInput">

    

    <fragment
        android:id="@+id/DataInput"
        android:name="com.example.about.DataInput"
        android:label="Data Input"
        tools:layout="@layout/fragment_data_input" >
        <action
            android:id="@+id/action_dataInput5_to_report2"
            app:destination="@id/MarksReport" />
    </fragment>
    <fragment
        android:id="@+id/MarksReport"
        android:name="com.example.about.Report"
        android:label="Marks Report"
        tools:layout="@layout/fragment_report" >
        <action
            android:id="@+id/action_report2_to_dataInput5"
            app:destination="@id/DataInput"
            app:popUpTo="@id/DataInput" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.example.about.AboutFragment"
        android:label="About"
        tools:layout="@layout/fragment_about" />
</navigation>